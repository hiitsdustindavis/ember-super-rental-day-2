
  <ul class="rental-tile">
    {{#each model.rentals as |rental|}}
    {{rental-tile rental=rental}}
    {{/each}}
  </ul>

  {{new-rental save2="save3"}}

  <h2> Cities </h2>

  <ul>
    {{#each model.cities as |city|}}
      <li>{{city.name}}</li>
    {{/each}}
  </ul>

  <h2>Announcements</h2>
  <ul>
    {{#each model.announcements as |announcement|}}
    {{announcement-tile announcement=announcement destroyAnnouncement="destroyAnnouncement"}}
    {{/each}}
  </ul>

  {{new-announcement emitSave="saveAnnouncement"}}
